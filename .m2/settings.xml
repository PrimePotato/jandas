<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <localRepository>D:\maven-repository</localRepository>
  <pluginGroups>
    <pluginGroup>org.sonarsource.scanner.maven</pluginGroup>
  </pluginGroups>
  <!--proxies>
    <proxy>
      <active>true</active>
      <protocol>https</protocol>
      <host>isapr</host>
      <port>80</port>
      <nonProxyHosts>artifactory.corp.lch.com|swpipx1.unix.lch.com</nonProxyHosts>
    </proxy>
  </proxies-->
  <profiles>
    <profile>
      <repositories>
        <repository>
          <id>repo1-cache</id>
          <name>repo1-cache</name>
          <url>http://artifactory.corp.lch.com:8091/artifactory/repo1-cache</url>
        </repository>
        <repository>
          <id>central</id>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <name>libs-releases</name>
          <url>http://artifactory.corp.lch.com:8091/artifactory/libs-releases</url>
        </repository>
        <repository>
          <id>snapshots</id>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
          </snapshots>
          <name>libs-snapshots</name>
          <url>http://artifactory.corp.lch.com:8091/artifactory/libs-snapshots</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>central</id>
          <name>plugins-releases</name>
          <url>http://artifactory.corp.lch.com:8091/artifactory/plugins-releases</url>
        </pluginRepository>
        <pluginRepository>
          <snapshots/>
          <id>snapshots</id>
          <name>plugins-snapshots</name>
          <url>http://artifactory.corp.lch.com:8091/artifactory/plugins-snapshots</url>
        </pluginRepository>
      </pluginRepositories>
      <properties>
        <!-- my.build.directory>d:\mavenbuild\${project.groupId}-${project.artifactId}\target</my.build.directory -->
      </properties>
      <id>artifactory</id>
    </profile>
    <profile>
      <id>sonar</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <sonar.host.url>http://swpipx1.unix.lch.com:9000/</sonar.host.url>
        <sonar.login>admin</sonar.login>
        <sonar.password>admin</sonar.password>
      </properties>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>artifactory</activeProfile>
  </activeProfiles>
</settings>
